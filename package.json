{
  "name": "fpp",
  "displayName": "FPPTools",
  "description": "F Prime Prime Language Support for Visual Studio Code",
  "version": "0.0.1",
  "publisher": "NASA JPL, in collaboration with UNLV CS",
  "contributors": [
    "See contributors.txt"
  ],
  "license": "Apache License 2.0",
  "repository": {
    "type": "git",
    "url": "To be added"
  },
  "homepage": "",
  "engines": {
    "vscode": "^1.64.0"
  },
  "categories": [
    "Programming Languages",
    "Formatters",
    "Other"
  ],
  "keywords": [
    "fpp",
    "f''",
    "f``",
    "f prime prime",
    "f prime"
  ],
  "preview": true,
  "activationEvents": [
    "onLanguage:fpp"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "languages": [
      {
        "id": "fpp",
        "aliases": [
          "FPP",
          "fpp",
          "F``",
          "f``",
          "F Prime Prime",
          "f prime prime"
        ],
        "extensions": [
          ".fpp",
          ".fppi"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "commands": [
      {
        "command": "fpptools.check",
        "title": "Check Semantics",
        "category": "FPP Tools"
      }
    ],
    "grammars": [
      {
        "language": "fpp",
        "scopeName": "source.fpp",
        "path": "./syntaxes/fpp.tmLanguage.json",
        "embeddedLanguages": {
          "meta.embedded.block.cpp": "cpp"
        }
      }
    ],
    "semanticTokenTypes": [
      {
        "id": "annotation",
        "superType": "decorator",
        "description": "FPP pre- and post- annotation."
      },
      {
        "id": "instance",
        "superType": "struct",
        "description": "FPP component instance."
      },
      {
        "id": "port",
        "superType": "variable",
        "description": "FPP endpoint of a connection between components."
      },
      {
        "id": "topology",
        "superType": "class",
        "description": "FPP set of component instances and the connections between their ports."
      },
      {
        "id": "component",
        "superType": "class",
        "description": "FPP unit of function."
      }
    ],
    "semanticTokenModifiers": [
      {
        "id": "component-kind",
        "description": "FPP component modifier."
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.2.0",
    "@types/mocha": "^9.0.0",
    "@types/node": "^14.18.10",
    "@types/vscode": "^1.64.0",
    "@typescript-eslint/eslint-plugin": "^5.9.1",
    "@typescript-eslint/parser": "^5.9.1",
    "@vscode/test-electron": "^2.0.3",
    "eslint": "^8.6.0",
    "glob": "^7.2.0",
    "mocha": "^9.1.3",
    "typescript": "^4.5.4"
  }
}