{
  "name": "fpp",
  "displayName": "F Prime Prime",
  "description": "F Prime Prime for Visual Studio Code",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.64.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:fpp.example",
    "onLanguage:fpp"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "fpp.example",
        "title": "F``: Example"
      }
    ],
    "languages": [
      {
        "id": "fpp",
        "aliases": [
          "FPP",
          "fpp",
          "F``",
          "f``",
          "F Prime Prime",
          "f prime prime"
        ],
        "extensions": [
          ".fpp"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "fpp",
        "scopeName": "source.fpp",
        "path": "./syntaxes/fpp.tmLanguage.json"
      }
    ],
    "semanticTokenTypes": [
      {
        "id": "Test",
        "description": "TEST"
      }
    ],
    "semanticTokenScopes": [
      {
        "scopes": {
          "comment": [
            "comment.line.number-sign.fpp"
          ]
        }
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.64.0",
    "@types/glob": "^7.2.0",
    "@types/mocha": "^9.0.0",
    "@types/node": "14.x",
    "@typescript-eslint/eslint-plugin": "^5.9.1",
    "@typescript-eslint/parser": "^5.9.1",
    "eslint": "^8.6.0",
    "glob": "^7.2.0",
    "mocha": "^9.1.3",
    "typescript": "^4.5.4",
    "@vscode/test-electron": "^2.0.3"
  }
}