{
  "name": "fpp",
  "displayName": "F Prime Prime",
  "description": "F Prime Prime for Visual Studio Code",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.64.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:fpp.example",
    "onLanguage:fpp"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "fpp.example",
        "title": "F``: Example"
      },
      {
        "command": "fpptools.check",
        "title": "Check Semantics",
        "category": "F`` Tools"
      }
    ],
    "languages": [
      {
        "id": "fpp",
        "aliases": [
          "FPP",
          "fpp",
          "F``",
          "f``",
          "F Prime Prime",
          "f prime prime"
        ],
        "extensions": [
          ".fpp"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "semanticTokenTypes": [
      {
        "id": "annotation",
        "superType": "decorator",
        "description": "FPP pre- and post- annotation."
      },
      {
        "id": "instance",
        "superType": "struct",
        "description": "FPP component instance."
      },
      {
        "id": "port",
        "superType": "variable",
        "description": "FPP endpoint of a connection between components."
      },
      {
        "id": "topology",
        "superType": "class",
        "description": "FPP set of component instances and the connections between their ports."
      },
      {
        "id": "component",
        "superType": "class",
        "description": "FPP unit of function."
      }
    ],
    "semanticTokenModifiers": [
      {
        "id": "component-kind",
        "description": "FPP component modifier."
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.2.0",
    "@types/mocha": "^9.0.0",
    "@types/node": "^14.18.10",
    "@types/vscode": "^1.64.0",
    "@typescript-eslint/eslint-plugin": "^5.9.1",
    "@typescript-eslint/parser": "^5.9.1",
    "@vscode/test-electron": "^2.0.3",
    "eslint": "^8.6.0",
    "glob": "^7.2.0",
    "mocha": "^9.1.3",
    "typescript": "^4.5.4"
  }
}